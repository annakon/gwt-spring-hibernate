<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <g:HTMLPanel>
        <div class="row">
            <div class="col-lg-6">
                <g:Button ui:field='btnAdd' styleName="btn btn-default">Добавление</g:Button>
            </div>
            <div class="col-lg-6">
                <div class="input-group">
                    <g:TextBox ui:field="search" styleName="form-control"/>
                    <span class="input-group-btn">
                        <g:Button ui:field='btnSearch' styleName="btn btn-default">Поиск</g:Button>
                    </span>
                </div>
            </div>
        </div>

    </g:HTMLPanel>
</ui:UiBinder>